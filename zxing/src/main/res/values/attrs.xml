<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 标题 -->
    <declare-styleable name="TitleView">
        <attr name="tv_title" format="string" />
        <attr name="tv_right" format="string" />
        <attr name="tv_img_left_visibility" format="enum">
            <enum name="visable" value="0"></enum>
            <enum name="invisable" value="1"></enum>
            <enum name="gone" value="2"></enum>
        </attr>
        <attr name="tv_img_right_visibility" format="enum">
            <enum name="visable" value="0"></enum>
            <enum name="invisable" value="1"></enum>
            <enum name="gone" value="2"></enum>
        </attr>
        <attr name="tv_right_tv_visibility" format="enum">
            <enum name="visable" value="0"></enum>
            <enum name="invisable" value="1"></enum>
            <enum name="gone" value="2"></enum>
        </attr>
    </declare-styleable>
</resources>
