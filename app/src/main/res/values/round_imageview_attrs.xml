<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!--这个是项目中用到的圆形头像view的自定义属性-->
    <declare-styleable name="RoundImageView">
    	<attr name="border_width" format="reference|dimension" />
    	<attr name="border_radius" format="reference|dimension"/>
    	<attr name="border_color" format="reference|color"/>
    	<attr name="hoverColor" format="reference|color"/>
    	<attr name="border_overlay" format="boolean"/>
    </declare-styleable>
</resources>
